<% include ./partials/header %>
<div class="masonry-layout-content">
    <div class="entries">
        <h1>Share this link for everyone to take your quiz!🔥<br><a href="http://besto.ga/<%= username %>">besto.ga/<%= username %></a></h1>
        <% if (entries == "[]") { %>
            No one has filled out your quiz...yet ;)
        <% } else { %>
            <% for (var i = 0; i < entries.length; i++) { %>
                <div class="entry">
                    <span><%= entries[i].message %></span> was 
                    <% if(scores[i] == "100") { %>
                        💯% right! Friendship confirmed!😄
                    <% } else { %>
                        <%= scores[i] %>% right!
                    <% } %>
                </div>
            <% } %>
        <% } %>
    </div>
    <div class="current-quiz-setting">
        <h1>Current Quiz Setting:</h1> <hr>
        Color: <%= currentSetting[0].color %><br>
        Age: <%= currentSetting[0].currentage %><br>
        Crush😍: <%= currentSetting[0].crush %><br>
        Movie: <%= currentSetting[0].movie %><br>
        Pet: <%= currentSetting[0].pet %><br>
        Snack: <%= currentSetting[0].snack %><br>
        Enemy😡: <%= currentSetting[0].enemy %><br>
        Favorite song💽: <%= currentSetting[0].favsong %><br>
        Song that'll make you cry: <%= currentSetting[0].sadsong %><br>
        Message: <%= currentSetting[0].message %><br>
        <a href="http://besto.ga/quizeditor/<%= username %>">Edit your quiz!</a><br>
    </div>
    <div class="clearfloat"></div>
</div>
<% include ./partials/footer %>

